<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <navigation-rule>
  <from-view-id>/admin/index.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/admin/dashboard.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
  <navigation-rule>
  <from-view-id>/admin/changepassword.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>saved</from-outcome>
   <to-view-id>/admin/manageusers.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
  <navigation-rule>
  <from-view-id>/admin/changepassword.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>failed</from-outcome>
   <to-view-id>/admin/changepassword.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/admin/ordernumbers.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>updated</from-outcome>
   <to-view-id>/admin/manageorders.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/admin/manageproducts.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>index</from-outcome>
   <to-view-id>/admin/manageproducts.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <message-bundle>resources.application</message-bundle>
  <locale-config>
   <default-locale>en</default-locale>
  </locale-config>
 </application>
</faces-config>
